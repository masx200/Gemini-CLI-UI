{
  "name": "@masx200/easy-llm-cli-ui",
  "version": "1.6.1",
  "description": "A stylish web-based UI for Gemini CLI",
  "type": "module",
  "main": "server/index.js",
  "scripts": {
    "watch": "tsc --watch",
    "watch:server": "tsc --watch -p tsconfig-server.json",
    "dev": "concurrently --kill-others \"npm run server\" \"npm run client\" \"npm run watch\" \"npm run watch:server\"",
    "server": "node server/index.js",
    "client": "vite --host",
    "build": "  tsc -p tsconfig-server.json && vite build && tsc",
    "preview": "vite preview",
    "start": "npm run build && npm run server"
  },
  "keywords": [
    "gemini",
    "ai",
    "code",
    "ui",
    "assistant"
  ],
  "author": "Gemini CLI UI Contributors",
  "license": "MIT",
  "dependencies": {
    "@codemirror/lang-css": "^6.3.1",
    "@codemirror/lang-html": "^6.4.9",
    "@codemirror/lang-javascript": "^6.2.4",
    "@codemirror/lang-json": "^6.0.1",
    "@codemirror/lang-markdown": "^6.3.3",
    "@codemirror/lang-python": "^6.2.1",
    "@codemirror/theme-one-dark": "^6.1.2",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@google/generative-ai": "^0.21.0",
    "@mui/material": "^7.3.1",
    "@tailwindcss/typography": "^0.5.16",
    "@uiw/react-codemirror": "^4.24.2",
    "@xterm/addon-clipboard": "^0.1.0",
    "@xterm/addon-webgl": "^0.18.0",
    "bcrypt": "^6.0.0",
    "better-sqlite3": "^12.2.0",
    "chokidar": "^4.0.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.515.0",
